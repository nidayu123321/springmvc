CREATE TABLE `gjj_user_info` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `name` varchar(50) COLLATE utf8_bin DEFAULT NULL,
  `companyName` varchar(255) COLLATE utf8_bin DEFAULT NULL,
  `personalAccount` varchar(100) COLLATE utf8_bin DEFAULT NULL,
  `companyAccount` varchar(100) COLLATE utf8_bin DEFAULT NULL,
  `idCard` varchar(100) COLLATE utf8_bin DEFAULT NULL,
  `cardNumber` varchar(100) COLLATE utf8_bin DEFAULT NULL,
  `wageBase` double DEFAULT NULL,
  `companyDepositRadio` varchar(50) COLLATE utf8_bin DEFAULT NULL,
  `personalDepositRadio` varchar(50) COLLATE utf8_bin DEFAULT NULL,
  `personalMonthlyPayment` double DEFAULT NULL,
  `companyMonthlyPayment` double DEFAULT NULL,
  `accountBalance` double DEFAULT NULL,
  `accountStatus` varchar(100) COLLATE utf8_bin DEFAULT NULL,
  `joinBank` varchar(255) COLLATE utf8_bin DEFAULT NULL,
  `collectionPoint` varchar(255) COLLATE utf8_bin DEFAULT NULL,
  `bankCard` varchar(100) COLLATE utf8_bin DEFAULT NULL,
  `password` varchar(50) COLLATE utf8_bin DEFAULT NULL,
  `officeBelone` varchar(255) COLLATE utf8_bin DEFAULT NULL,
  `dateOpened` date COLLATE utf8_bin DEFAULT NULL,
  `monthlyPayment` double COLLATE utf8_bin DEFAULT NULL,
  `lastYearBalance` double COLLATE utf8_bin DEFAULT NULL,
  PRIMARY KEY (`id`),
  KEY `index_name` (`personalAccount`),
  KEY `INDEX_IDCARD` (`idCard`),
  KEY `INDEX_PERSONALACCUNT` (`personalAccount`)
) ENGINE=InnoDB AUTO_INCREMENT=3 DEFAULT CHARSET=utf8 COLLATE=utf8_bin;